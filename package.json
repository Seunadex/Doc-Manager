{
  "name": "doc-manager",
  "version": "1.0.0",
  "description": "Document manager",
  "main": "server.js",
  "scripts": {
    "postinstall": "gulp dev",
    "start": "node build/server.js",
    "start:dev": "nodemon ./server/server.js --exec babel-node --presets es2015,stage-2",
    "dev": "NODE_ENV=test nodemon --watch server --exec babel-node -- server/server.js",
    "test": "NODE_ENV=production gulp dev &&  gulp coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "coverage": "gulp coverage",
    "testmigrate": "NODE_ENV=test sequelize db:migrate",
    "undo": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Seunadex/Doc-Manager.git"
  },
  "author": "Seun Adekunle",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Seunadex/Doc-Manager/issues"
  },
  "homepage": "https://github.com/Seunadex/Doc-Manager#readme",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-validator": "^3.2.1",
    "faker": "^4.1.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-exit": "0.0.2",
    "gulp-inject-modules": "^1.0.0",
    "gulp-istanbul": "^1.1.2",
    "gulp-jasmine": "^2.4.2",
    "gulp-jasmine-node": "^1.0.7",
    "gulp-nodemon": "^2.2.1",
    "jsonwebtoken": "^7.4.1",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "path": "^0.12.7",
    "pg": "^6.4.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.3.2",
    "sequelize-cli": "^2.7.0",
    "validator": "^8.0.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "coveralls": "^2.13.1",
    "nodemon": "^1.11.0",
    "supertest": "^3.0.0"
  }
}
